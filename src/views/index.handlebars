<div>
    <h1>
        E-Commerce de tecnología
    </h1>
</div>
{{#each products}}
    <div class="product">
      {{#each thumbnails}}
        <img src={{this}} alt="Imagen del producto">
      {{/each}}
      <h2>{{title}}</h2>
      <p class="description">{{description}}</p>
      <p>Precio: ${{price}}</p>
      <p>Stock:{{stock}}</p>
      <button class="add-to-cart">Añadir al carrito</button>
    </div>
{{/each}}
